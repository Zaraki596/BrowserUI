<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools">
    <data>

    </data>
    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        <TextView
                android:text="Open Source License Credit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tv_open_so_title" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="52dp" app:layout_constraintBottom_toTopOf="@+id/tv_open_sour_sub"
                app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="8dp"
                app:layout_constraintVertical_bias="1.0" android:fontFamily="@font/roboto" android:textColor="#000"/>
        <TextView
                android:text="MagTapp 1.0"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tv_open_sour_sub" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="52dp" app:layout_constraintBottom_toTopOf="@+id/divider3"
                android:layout_marginBottom="8dp"/>
        <View
                android:id="@+id/divider3"
                android:layout_width="0dp"
                android:layout_height="1dp"
                android:background="?android:attr/listDivider"
                app:layout_constraintBottom_toTopOf="@+id/tv_copyrt_title"
                android:layout_marginBottom="8dp" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"/>
        <TextView
                android:text="Copyright Notices"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tv_copyrt_title" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="52dp"
                app:layout_constraintBottom_toTopOf="@+id/tv_copyryt_sub" android:fontFamily="@font/roboto"
                android:textColor="#000"/>
        <TextView
                android:text="Let us know if your content is used inaprropriately"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tv_copyryt_sub" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="52dp" app:layout_constraintBottom_toTopOf="@+id/divider4"
                android:layout_marginBottom="8dp"/>
        <View
                android:id="@+id/divider4"
                android:layout_width="0dp"
                android:layout_height="1dp"
                android:background="?android:attr/listDivider"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintBottom_toTopOf="@+id/tv_privacy_notice"
                android:layout_marginBottom="8dp"/>
        <TextView
                android:text="Privacy Notice"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tv_privacy_notice" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="52dp"
                android:textColor="#000" android:fontFamily="@font/roboto"
                app:layout_constraintBottom_toTopOf="@+id/divider6" android:layout_marginBottom="8dp"/>
        <View
                android:id="@+id/divider6"
                android:layout_width="0dp"
                android:layout_height="1dp"
                android:background="?android:attr/listDivider"
                app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintBottom_toTopOf="@+id/tv_terms_condtn"
                android:layout_marginBottom="8dp"/>
        <TextView
                android:text="Terms and Condition"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tv_terms_condtn" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="52dp" app:layout_constraintBottom_toTopOf="@+id/divider5"
                android:textColor="#000" android:fontFamily="@font/roboto" android:layout_marginBottom="8dp"/>
        <View
                android:id="@+id/divider5"
                android:layout_width="0dp"
                android:layout_height="1dp"
                android:background="?android:attr/listDivider"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="542dp"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>